[program:vendor-app]
command = /home/ubuntu/gunicorn_conf/gunicorn_vendor                  ; Command to start app
user = ubuntu                                                         ; User to run as
stdout_logfile = /home/ubuntu/gunicorn_conf/logs/gunicorn_supervisor.log  ; Where to write log messages
redirect_stderr = true                                                ; Save stderr in the same log
environment=LANG=en_US.UTF-8,LC_ALL=en_US.UTF-8                       ; Set UTF-8 as default encoding]
